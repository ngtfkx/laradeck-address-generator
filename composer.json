{
    "name": "ngtfkx/laradeck-address-generator",
    "type": "library",
    "description": "Generating real addresses to create test data",
    "keywords": [
        "ngtfkx",
        "LaradeckAddressGenerator",
        "address",
        "fixtures",
        "data",
        "laradeck"
    ],
    "homepage": "https://github.com/ngtfkx/laradeck-address-generator",
    "license": "MIT",
    "authors": [
        {
            "name": "Denis Sandal",
            "email": "den.sandal@gmail.com",
            "homepage": "https://github.com/ngtfkx/",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : "~7.1"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=5.4.3",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Ngtfkx\\LaradeckAddressGenerator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ngtfkx\\LaradeckAddressGenerator\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
